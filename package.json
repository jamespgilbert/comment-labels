{
    "name": "comment-labels",
    "displayName": "Comment Labels",
    "description": "Create large text comments for easy visual segmentation of code",
    "version": "0.1.0",
    "publisher": "jamespgilbert",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.commentLabel"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.commentLabel",
                "title": "Comment Label"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/comment-labels.git"
    }
}